let poteCafe = "café pilão";
let poteAcucar = "açúcar cristal" ;
let poteBiscoito = "biscoito maizena";

const mensagemDaVovo = "Nos potes da vovó tem: ";

// alteração no valor da variável: poteCafe! 
poteCafe = "café 3 corações";

console.log(mensagemDaVovo + 
poteCafe + ", " + 
poteAcucar + " e " + 
poteBiscoito + ".");
